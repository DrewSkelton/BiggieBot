    "mathjs": "^14.9.1",
    "node-cron": "^3.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@types/node": "^22.19.2",
    "@types/node-cron": "^3.0.11",
    "drizzle-kit": "^0.31.8",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.49.0"
